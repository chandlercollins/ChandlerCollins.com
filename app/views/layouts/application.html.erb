}

figure.hover_select.done div.filled {
	display: none;
}

figure.hover_select div.filled {
	position: absolute;
	background-color: #000;
	width: 40px;
	height: 40px;
	-webkit-border-radius: 40px;
	-moz-border-radius: 40px;
	border-radius: 40px;
	top: 0px;
	-webkit-transform: scale(0.2);
	-moz-transform: scale(0.2);
}


figure.hover_select.poke div.filled {
	position: absolute;
	background-color: #5b9819;
	width: 100px;
	height: 100px;
	-webkit-border-radius:100px;
	-moz-border-radius: 100px;
	border-radius: 100px;
	top: 0px;
	-webkit-transform: scale(0.2);
	-moz-transform: scale(0.2);
}

figure.hover_select.animate div.filled {
	-webkit-animation-name: hoverPulse;
	-moz-animation-name: hoverPulse;
	-webkit-animation-duration: 3s;
	-moz-animation-duration: 3s;
	-webkit-animation-iteration-count: infinite;
	-moz-animation-iteration-count: infinite;
	-webkit-animation-timing-function:linear;
	-moz-animation-timing-function: linear;
}

figure.hover_select:hover div.filled {
  -webkit-animation-name: hoverSelect;
  -moz-animation-name: hoverSelect;
	-webkit-animation-duration: 1s;
	-moz-animation-duration: 1s;
	-webkit-animation-iteration-count: 1;
	-moz-animation-iteration-count: 1;
